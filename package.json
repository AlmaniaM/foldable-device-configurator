{
  "name": "foldable-device-configurator",
  "version": "1.0.0",
  "description": "A simple web component which you can add to emulate various type of foldables devices.",
  "main": "src/index.js",
  "snowpack": {
    "webDependencies": [
      "lit-element",
      "lit-html",
      "lit-html/directives/class-map.js"
    ],
    "dedupe": [
      "lit-element",
      "lit-html"
    ]
  },
  "scripts": {
    "start": "es-dev-server --node-resolve --watch --open demo/index.html --port 8081",
    "build": "rimraf web_modules && snowpack --optimize"
  },
  "dependencies": {
    "@material/mwc-button": "^0.12.0",
    "@material/mwc-icon-button": "^0.12.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2"
  },
  "devDependencies": {
    "es-dev-server": "^1.36.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "rollup": "^1.29.0",
    "rollup-plugin-re": "^1.0.7",
    "snowpack": "^1.0.5",
    "rimraf": "^3.0.0"
  },
  "keywords": [],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alexis Menard",
    "email": "alexis.menard@intel.com"
  }
}